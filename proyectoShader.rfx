     <RmGLShader NAME="Vertex Program" API="OpenGL" PIXEL_SHADER="FALSE"><RmShaderCode TYPE="TEXT"><![CDATA[
// .----------------------------------------------------------------------.
// UNIVERSIDAD SIMÓN BOLÍVAR.
// ALUMNO:
//    Edward Fernández - Carnet: 10-11121

// .----------------------------------------------------------------------.

// Para las texturas.
varying vec2 texCoord;

// Para la camara y la luz.
varying vec3 vN;  // Vector Normal.
varying vec3 cLp; // Color Luz puntual.
varying vec3 vC;  // Vector de la camara.
varying vec4 vPos;// Posición de la camara.
varying float distanciaIlum;

// Booleano para testear el bump mapping.
uniform bool useNormalMap; 

// Vectores tangente y bitangente de la superficie.
attribute vec3 vtangente, vbitangente;

void main(void)
{
   
   vec4 lp; // Luz puntual.
   
   // Obtengo el vector normal y la luz puntual.
   vN = gl_NormalMatrix * gl_Normal;
   lp = gl_ModelViewMatrix * vec4(1.0,1.0,1.0,0.0);
   
   // Posición de la camara.
   vPos = gl_ModelViewMatrix * gl_Vertex;
   
   distanciaIlum = length(lp.xyz - vPos.xyz); 
   
   // Tomo las coordenadas para las texturas.
   texCoord = gl_MultiTexCoord0.st;

   // Espacio tangente.
   vec3 camara = -vPos.xyz;
   vec3 tangente   = gl_NormalMatrix * vtangente;
   vec3 bitangemte = gl_NormalMatrix * vbitangente;
   
   if (useNormalMap){
      vC.x = dot(tangente, camara);
      vC.y = dot(bitangemte, camara);
      vC.z = dot(vN, camara);
      
      // Le paso el color de la luz.
      cLp.x = dot(tangente, lp.xyz);
      cLp.y = dot(bitangemte, lp.xyz);
      cLp.z = dot(vN, lp.xyz);
   }else{
      vC = camara;
      
      // Le paso el color de la luz.
      cLp = lp.xyz;
   } 

   gl_Position = ftransform();
}]]></RmShaderCode></RmGLShader>
     <RmGLShader NAME="Fragment Program" API="OpenGL" PIXEL_SHADER="TRUE"><RmShaderCode TYPE="TEXT"><![CDATA[
// .----------------------------------------------------------------------.
// UNIVERSIDAD SIMÓN BOLÍVAR.
// ALUMNO:
//    Edward Fernández - Carnet: 10-11121

// .----------------------------------------------------------------------.
// VARIABLES A UTILIZAR:

// Intensidades.
uniform float iL, iA, iSh, iS, iD;

// Efectos.
uniform float snow, moho;

// Colores.
uniform vec4 cL, cA, cS, cM;

// Componentes.
uniform vec4 shine;

// Texturas.
uniform sampler2D terreno_color, terreno_normal, terreno_moho;
uniform sampler2D terreno_snow, terreno_color_snow, terreno_snow_normal;
uniform sampler2D terreno_shadow, terreno_specular;
varying vec2 texCoord;

// Camara y luz.
varying vec3 vN, cLp, vC;
varying vec4 vPos;
varying float distanciaIlum;

// Para decidir cuando usar el normal mapping.
uniform bool useNormalMap;

// .----------------------------------------------------------------------.

void main(void)
{
   vec4 colorDifuso;
   float dotLN; // Producto punto entre la luz puntual y la normal.

   // Se recuperará la información de las texturas.
   vec2 repeatUV;
   
   repeatUV.s = texCoord.s;
   repeatUV.t = texCoord.t;
   
   // Se cargan las texturas.
   vec4 cTerreno_color = texture2D(terreno_color,repeatUV);
   vec3 cTerreno_normal = (texture2D(terreno_normal,repeatUV).xyz *2.0) - 1.0;
   vec4 cTerreno_moho = texture2D(terreno_moho,repeatUV);
   
   vec4 cTerreno_snow = texture2D(terreno_snow,repeatUV);
   vec4 cTerreno_color_snow = texture2D(terreno_color_snow,repeatUV);
   vec4 cTerreno_snow_normal = texture2D(terreno_snow_normal,repeatUV);
   
   vec4 cTerreno_shadow = texture2D(terreno_shadow,repeatUV);
   vec4 cTerreno_specular = texture2D(terreno_specular,repeatUV);

   vec3 N; // Vector normal.
   
   if (useNormalMap){
     N = normalize(cTerreno_normal);
   }else{
     N = normalize(vN);
   }
   
   // Componente difuso.
   dotLN = max(dot(normalize(cLp), N), 0.0);
   vec4 difuso = dotLN * cTerreno_color * iD;

   // Componente Ambiental.
   vec4 ambiental = cA * cTerreno_color * iA;

   // Componente especular.
   vec4 shine = vec4(0.0, 0.0, 0.0, 0.0);
   vec3 r = reflect(-normalize(cLp), N); // Vector de reflexión.
   // Producto punto entre el vector de reflexión y la camara.
   float dotRC = pow(max(dot(normalize(r),normalize(vC)),0.0),iS);
   shine = iS* cTerreno_specular * cS * dotRC;
    
   float atenuacion = 1.0 / (1.0 + iL * pow(distanciaIlum, 2));
   
   vec4 colorFinal = ambiental + iL*(cL*(difuso + shine));
   float gamma = 1.0 / 2.2;
   //gl_FragColor = vec4(pow(colorFinal.x,gamma), pow(colorFinal.y,gamma), pow(colorFinal.z,gamma), 1);
   gl_FragColor = colorFinal;
}]]></RmShaderCode></RmGLShader>
